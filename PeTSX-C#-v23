function GetCoinsSuperArea(area)
    local Areas = (IMightKillMyselfCauseOfThis)
    local returntable = {}
    local ListCoins = game.workspace['__THINGS']['__REMOTES']["get coins"]:InvokeServer({})[1]
    for i,v in pairs(ListCoins) do
        if getgenv().SelecArea == 'All' or table.find(Areas[getgenv().SelecArea], v.a) then
            local Coin = v
            Coin["index"] = i
            table.insert(returntable, Coin)
         end
    end
    return returntable
end

