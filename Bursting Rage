        end)
    end
end)

local npctp = {}
for i,v in pairs(game:GetService("Workspace").Map.NPCs:GetChildren()) do
    if v:IsA("Model") then
        table.insert(npctp,v.Name)
    end
end

TP:Dropdown("select NPC", npctp, function(v)
SlectedNPC = v
end)

TP:Button("TP no selected NPC", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.NPCs[SlectedNPC].Head.CFrame
end)

Credits:Button("Mana#9724", function()
setclipboard("Mana#9724")
end)

Credits:Button("Discord Server", function()
    setclipboard("https://discord.gg/YFDHGtXgJV")
end)
